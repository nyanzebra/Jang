#pragma once

namespace jang {	
	
constexpr long double operator"" _litre_type(long double value)  { return value; }

constexpr long double operator"" _deci_litre_type(long double value) { return operator"" _litre_type(value) / 10; }
constexpr long double operator"" _centi_litre_type(long double value) { return operator"" _deci_litre_type(value) / 10; }
constexpr long double operator"" _milli_litre_type(long double value) { return operator"" _centi_litre_type(value) / 10; }
constexpr long double operator"" _micro_litre_type(long double value) { return operator"" _milli_litre_type(value) / 10; }
constexpr long double operator"" _nano_litre_type(long double value) { return operator"" _micro_litre_type(value) / 10; }
constexpr long double operator"" _pico_litre_type(long double value) { return operator"" _nano_litre_type(value) / 10; }
constexpr long double operator"" _femto_litre_type(long double value) { return operator"" _pico_litre_type(value) / 10; }
constexpr long double operator"" _atto_litre_type(long double value) { return operator"" _femto_litre_type(value) / 10; }
constexpr long double operator"" _zepto_litre_type(long double value) { return operator"" _atto_litre_type(value) / 10; }
constexpr long double operator"" _yocto_litre_type(long double value) { return operator"" _zepto_litre_type(value) / 10; }

constexpr long double operator"" _deka_litre_type(long double value) { return operator"" _litre_type(value) * 10; }
constexpr long double operator"" _hecto_litre_type(long double value) { return operator"" _deka_litre_type(value) * 10; }
constexpr long double operator"" _kilo_litre_type(long double value) { return operator"" _hecto_litre_type(value) * 10; }
constexpr long double operator"" _mega_litre_type(long double value) { return operator"" _kilo_litre_type(value) * 10; }
constexpr long double operator"" _giga_litre_type(long double value) { return operator"" _mega_litre_type(value) * 10; }
constexpr long double operator"" _tera_litre_type(long double value) { return operator"" _giga_litre_type(value) * 10; }
constexpr long double operator"" _peta_litre_type(long double value) { return operator"" _tera_litre_type(value) * 10; }
constexpr long double operator"" _exa_litre_type(long double value) { return operator"" _peta_litre_type(value) * 10; }
constexpr long double operator"" _zetta_litre_type(long double value) { return operator"" _exa_litre_type(value) * 10; }
constexpr long double operator"" _yotta_litre_type(long double value) { return operator"" _zetta_litre_type(value) * 10; }

constexpr long double operator"" _litre_type(unsigned long long int value)  { return value; }

constexpr long double operator"" _deci_litre_type(unsigned long long int value) { return operator"" _litre_type(value) / 10; }
constexpr long double operator"" _centi_litre_type(unsigned long long int value) { return operator"" _deci_litre_type(value) / 10; }
constexpr long double operator"" _milli_litre_type(unsigned long long int value) { return operator"" _centi_litre_type(value) / 10; }
constexpr long double operator"" _micro_litre_type(unsigned long long int value) { return operator"" _milli_litre_type(value) / 10; }
constexpr long double operator"" _nano_litre_type(unsigned long long int value) { return operator"" _micro_litre_type(value) / 10; }
constexpr long double operator"" _pico_litre_type(unsigned long long int value) { return operator"" _nano_litre_type(value) / 10; }
constexpr long double operator"" _femto_litre_type(unsigned long long int value) { return operator"" _pico_litre_type(value) / 10; }
constexpr long double operator"" _atto_litre_type(unsigned long long int value) { return operator"" _femto_litre_type(value) / 10; }
constexpr long double operator"" _zepto_litre_type(unsigned long long int value) { return operator"" _atto_litre_type(value) / 10; }
constexpr long double operator"" _yocto_litre_type(unsigned long long int value) { return operator"" _zepto_litre_type(value) / 10; }

constexpr long double operator"" _deka_litre_type(unsigned long long int value) { return operator"" _litre_type(value) * 10; }
constexpr long double operator"" _hecto_litre_type(unsigned long long int value) { return operator"" _deka_litre_type(value) * 10; }
constexpr long double operator"" _kilo_litre_type(unsigned long long int value) { return operator"" _hecto_litre_type(value) * 10; }
constexpr long double operator"" _mega_litre_type(unsigned long long int value) { return operator"" _kilo_litre_type(value) * 10; }
constexpr long double operator"" _giga_litre_type(unsigned long long int value) { return operator"" _mega_litre_type(value) * 10; }
constexpr long double operator"" _tera_litre_type(unsigned long long int value) { return operator"" _giga_litre_type(value) * 10; }
constexpr long double operator"" _peta_litre_type(unsigned long long int value) { return operator"" _tera_litre_type(value) * 10; }
constexpr long double operator"" _exa_litre_type(unsigned long long int value) { return operator"" _peta_litre_type(value) * 10; }
constexpr long double operator"" _zetta_litre_type(unsigned long long int value) { return operator"" _exa_litre_type(value) * 10; }
constexpr long double operator"" _yotta_litre_type(unsigned long long int value) { return operator"" _zetta_litre_type(value) * 10; }

}
