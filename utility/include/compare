#pragma once

#include "mathematics/operations"

namespace jang {

template <typename T>
struct equal { bool operator()(const T& lhs, const T& rhs) { return equal_to(lhs, rhs); } };

template <typename T>
struct less { bool operator()(const T& lhs, const T& rhs) { return less_than(lhs, rhs); } };

template <typename T>
struct greater { bool operator()(const T& lhs, const T& rhs) { return greater_than(lhs, rhs); } };

}
